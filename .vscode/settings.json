    "**/tsconfig.spec.json": false,
    "**/.prettierrc": false,
    "**/.prettierignore": false,
    "**/buildspec.yml": false,
    "**/.stylelintrc.json": false,
    "**/angular.json": false,
    "**/.gitignore": false,
    "**/.gitkeep": false,
    "tsconfig.app.json": false,
    "tsconfig.json": false,
    "tslint.json": false,
    "README.md": false,
    "src/favicon.ico": false,
    "src/main.ts": false,
